                "-r",
                "ts-node/register",
                "-r",
                "tsconfig-paths/register"
            ],
            "program": "${workspaceFolder}/node_modules/jest/bin/jest",
            "args": [
                "--runInBand",
                "--testPathPattern=test/unit/services/ario/ARIOService.test.ts",
                "--config=test/jest.config.js"
